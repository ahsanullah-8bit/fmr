{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "fmr",
  "maintainers": "Ahsan Ullah <ahsanmir358@gmail.com>",
  "description": "fmr is a light-weight utility library to ease the use of Computer Vision AI models running locally",
  "supports": "windows",
  "dependencies": [
    {
      "name": "opencv4",
      "default-features": false,
      "features": [ "fs", "ffmpeg", "eigen", "highgui", "jpeg", "png", "win32ui"],
      "version>=": "4.10.0"
    },
    {
      "name": "spdlog",
      "version>=": "1.15.3"
    },
    {
    "name": "argparse",
    "version>=": "3.2"
    },
    {
        "name": "yaml-cpp",
        "version>=": "0.8.0#2"
    },
    {
      "name": "gtest",
      "version>=": "1.16.0"
    }
  ]
}
