{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "framer",
  "maintainers": "Ahsan Ullah <CS120212021@kkkuk.edu.pk>",
  "description": "Framer is a light-weight library to ease the use of Computer Vision AI models running locally",
  "supports": "windows",
  "dependencies": [
    {
      "name": "opencv4",
      "default-features": false,
      "features": [ "fs", "gstreamer", "eigen", "highgui", "jpeg", "png" ],
      "version>=": "4.10.0"
    },
    {
      "name": "spdlog",
      "version>=": "1.15.3"
    },
    {
        "name": "yaml-cpp",
        "version>=": "0.8.0#2"
    },
    {
      "name": "gtest",
      "version>=": "1.16.0"
    }
  ]
}
