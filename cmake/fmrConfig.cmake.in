@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

if (@IS_MAIN_PROJECT@)
    # Is being built as a separate project and integrated
	find_dependency(onnxruntime REQUIRED)
else()
    # Is a sub-project, either through submodule'ing or FetchContent
	find_dependency(onnxruntime QUIET)
endif()

find_dependency(OpenCV 4.10.0 REQUIRED)
find_dependency(spdlog 1.15.3 REQUIRED)
find_dependency(yaml-cpp 0.8.0 REQUIRED)

include("${CMAKE_CURRENT_LIST_DIR}/fmrTargets.cmake")
check_required_components(fmr)
